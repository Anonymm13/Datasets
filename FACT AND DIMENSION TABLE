CREATE TABLE CUSTOMER_DIM (
    CUST_ID NUMBER PRIMARY KEY,
    CUST_NAME VARCHAR2(100),
    CUST_SSN NUMBER,
    CUST_AGE NUMBER,
    CUST_GNDR VARCHAR2(1),
    CUST_ADDRESS VARCHAR2(100),
    START_DATE DATE,
    END_DATE DATE
);




CREATE TABLE BRANCH_DIM (
    BR_ID NUMBER PRIMARY KEY,
    BR_NAME VARCHAR2(100),
    BR_LCTN VARCHAR2(50),
    BR_ADDRESS VARCHAR2(100),
    BR_MNGR VARCHAR2(100)
);





CREATE TABLE PRODUCT_DIM (
    PROD_ID NUMBER PRIMARY KEY,
    PROD_NAME VARCHAR2(100),
    CATEGORY VARCHAR2(50),
    PRICE NUMBER(10,2)
);





CREATE TABLE SALES_FACT (
    SL_ID NUMBER PRIMARY KEY,
    SL_CUST_ID NUMBER,
    SL_BRNCH_ID NUMBER,
    SL_PROD_ID NUMBER,
    SL_UNIT NUMBER,
    SL_AMNT NUMBER(10,2),
    SALE_TYPE VARCHAR2(20),
    SALE_DATE DATE,
    FOREIGN KEY (SL_CUST_ID) REFERENCES CUSTOMER_DIM(CUST_ID),
    FOREIGN KEY (SL_BRNCH_ID) REFERENCES BRANCH_DIM(BR_ID),
    FOREIGN KEY (SL_PROD_ID) REFERENCES PRODUCT_DIM(PROD_ID)
);








If Index(cust_address, "@") > 0 Or 
   Index(cust_address, "#") > 0 Or 
   Index(cust_address, "$") > 0 Then "Invalid" Else "Valid"


